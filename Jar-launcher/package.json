{
  "name": "jar-launcher",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --out-path dist/ --targets node18-macos-arm64,node18-macos-x64",
